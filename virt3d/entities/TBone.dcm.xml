<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="TBone" table="t_bone">
    <id name="boneid" type="integer" column="boneId">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="text" column="name" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="commonname" type="text" column="commonName" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="skeletaldivision" type="text" column="skeletalDivision" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="descriptionof" type="text" column="descriptionOf" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="location" type="text" column="location" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="numberof" type="text" column="numberOf" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="articulation" type="text" column="articulation" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="bonestatus" type="string" column="boneStatus" length="50" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">Draft</option>
      </options>
    </field>
    <field name="boneowner" type="string" column="boneOwner" length="50" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
